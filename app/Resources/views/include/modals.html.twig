<div class="modal fade" id="modal-delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel12" aria-hidden="true">
    <div class="modal-wrapper">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header bg-red">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel12">Eliminar</h4>
                </div>
                <div class="modal-body">
                    <p></p>
                </div>
                <div class="modal-footer">
                    <div class="btn-group">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                        <a href="#" type="button" class="btn btn-danger">Eliminar</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--Modal para agreagar marca en caso q no este en la bd-->
<div id="add-marca" class="md-modal md-effect-4" style="width: 20%">
    <div class="md-content modal-content">
        <div class="modal-header">
            <h4 id="myModalLabel22" class="modal-title">Agregar Marca</h4>
        </div>
        <div class="modal-body">
            <form action="#" method="post" role="form">
                <div class="form-group">
                    <label>Marca</label>
                    <input type="text" class="form-control" placeholder="Introdusca la marca">
                </div>
            </form>
        </div>
        <div class="modal-footer">
            <div class="btn-group">
                <button class="btn btn-default md-close" type="button" data-dismiss="modal">Cerrar</button>
                <button class="btn btn-primary" type="button">Agregar</button>
            </div>
        </div>
    </div>
</div>

<!--Modal para agreagar modelo en caso q no este en la bd-->
<div id="add-model" class="md-modal md-effect-4" style="width: 20%">
    <div class="md-content modal-content">
        <div class="modal-header">
            <h4 id="myModalLabel22" class="modal-title">Agregar Modelo</h4>
        </div>
        <div class="modal-body">
            <form action="#" method="post" role="form">
                <div class="form-group">
                    <label>Marca</label>
                    {{ render(controller('SystemBackendBundle:Mark:findAllMarcas')) }}
                </div>

                <div class="form-group">
                    <label>Modelo</label>
                    <input type="text" class="form-control" placeholder="Introdusca el modelo">
                </div>

            </form>
        </div>
        <div class="modal-footer">
            <div class="btn-group">
                <button class="btn btn-default md-close" type="button" data-dismiss="modal">Cerrar</button>
                <button class="btn btn-primary" type="button">Agregar</button>
            </div>
        </div>
    </div>
</div>